DROP TABLE KHPC;
DROP SEQUENCE KHPCSEQ;

CREATE SEQUENCE KHPCSEQ;

-- 번호 , 이름, 아이디, 패스워드, 생년월일, 전화번호, 주소, 가입날짜

CREATE TABLE KHPC(
	MYNO NUMBER NOT NULL,
	ID VARCHAR2(1000) NOT NULL,
	PW VARCHAR2(1000) NOT NULL,
	NAME VARCHAR2(1000) NOT NULL,
	DOB VARCHAR2(6) NOT NULL,
	TEL VARCHAR2(100) NOT NULL,
	ADDR VARCHAR2(4000) NOT NULL,
	JD DATE NOT NULL,
	CONSTRAINT KHPC_PK PRIMARY KEY(MYNO),
	CONSTRAINT KHPC_UQ_ID UNIQUE(ID)
);

INSERT INTO KHPC
(MYNO,ID,PW,NAME,DOB,TEL,ADDR,JD)
VALUES(KHPCSEQ.NEXTVAL,'aa','aa','정햇나라','921130','010-1234-5678','경기도 성남시',
SYSDATE);

SELECT MYNO,ID,PW,NAME,DOB,TEL,ADDR,JD
FROM KHPC;

SELECT * FROM KHPC;

